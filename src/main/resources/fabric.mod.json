{
  "schemaVersion": 1,
  "id": "subtick",
  "version": "${version}",

  "name": "SubTick",
  "description": "A carpet extension allowing for finer tick step control.",
  "authors": [
    "lntricate"
  ],
  "contributors": [
    "SpaceWalker"
  ],
  "contact": {
    "sources": "https://github.com/lntricate1/SubTick"
  },

  "license": "LGPLv3",
  "icon": "assets/subtick/icon.png",

  "environment": "*",
  "accessWidener": "subtick.accesswidener",

  "entrypoints": {
    "main": [
      "subtick.SubTick"
    ],
    "client": [
      "subtick.client.SubTickClient"
    ],
    "g4mespeed": [
      "subtick.g4mespeed.SubtickExtension"
    ],
    "modmenu": [
      "subtick.client.ModMenuImpl"
    ]
  },
  "mixins": [
    "subtick.mixins.json"
  ],

  "depends": {
    "fabricloader": ">=0.11.4",
    "minecraft": "${minecraft_dependency}",
    "carpet": "${carpet_dependency}"
  },

  "custom": {
    "modmenu": {
      "links": {
        "modmenu.discord": "https://discord.gg/vPyUBcdmZV"
      }
    }
  }
}
